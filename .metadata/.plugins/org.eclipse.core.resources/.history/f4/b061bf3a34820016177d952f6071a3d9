import java.util.Scanner;

  public class Anwser {

    public static void main(String[] args) {
	  Scanner in = new Scanner(System.in);
	  String str = in.nextLine();
//	  System.out.println("String is " + str);
//	  97 to 122
      String output = "a";
	  for (int i = 0; i < str.length(); i++) {
		  int charIndex = Character.getNumericValue(str.charAt(i));
		  System.out.println(charIndex);
		  if (charIndex > 10 && charIndex < 22 ) {			
			  output = output + (char) (Character.getNumericValue(str.charAt(i)) + 13 + 109);
		  } else if (charIndex > 22 && charIndex < 35){
			  output = output + (char) (Character.getNumericValue(str.charAt(i)) - 13 + 109);
		  }
		  System.out.println("output is " + output);
	  }
//	  System.out.println("char at 96 is " + (char) 97);
//	  System.out.println((char) 122);
//	  System.out.println((char) 109);
//	  System.out.println(output);
//	  System.out.println(Character.getNumericValue('m'));
    }
  }
