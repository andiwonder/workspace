import java.util.Scanner;

  public class Anwser {

    public static void main(String[] args) {
	  Scanner in = new Scanner(System.in);
	  String str = in.nextLine();
	  int z = 122;
	  int a = 97;
	  System.out.println(char (z));
	  System.out.println((char) a);
//	  System.out.println("String is " + str);
//	  97 to 122 (a -> z) , 10 to 35
      String output = "";
	  for (int i = 0; i < str.length(); i++) {
		  int charIndex = Character.getNumericValue(str.charAt(i));
		  System.out.println(charIndex);
		  if (charIndex > 22 && charIndex < 35 ) {			  
			  output = output + (char) (Character.getNumericValue(str.charAt(i)) + 97 - 10);
		  } else if (charIndex > 10 && charIndex < 22){
			  output = output + (char) (122 - Character.getNumericValue(str.charAt(i)) + 97 - 10);
		  }
		  System.out.println("output is " + output);
	  }
//	  System.out.println("char at 96 is " + (char) 97);
//	  System.out.println((char) 122);
//	  System.out.println((char) 109);
//	  System.out.println(output);
//	  System.out.println(Character.getNumericValue('m'));
    }
  }
